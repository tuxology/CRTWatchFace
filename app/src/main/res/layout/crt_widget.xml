<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout"
    android:layout_width="240px"
    android:layout_height="240px"
    android:background="@drawable/bg" >
    
    <ImageView
        android:id="@+id/time_hours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="26px"
        android:layout_y="30px" />

    <ImageView
        android:id="@+id/time_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="26px"
        android:layout_y="87px" />

    <ImageView
        android:id="@+id/day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="33px" />

    <ImageView
        android:id="@+id/month"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="63px" />

    <ImageView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="93px" />

    <ImageView
        android:id="@+id/line4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="123px" />

    <ImageView
        android:id="@+id/line5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="153px" />

    <ImageView
        android:id="@+id/line6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="183px" />

    <ImageView
        android:id="@+id/line7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="122px"
        android:layout_y="213px" />

</AbsoluteLayout>
